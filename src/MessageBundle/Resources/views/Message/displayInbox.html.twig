{% extends "@User/layout.html.twig" %}
{% block main %}
    <script>
        function resizeIframe(obj) {
            obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';

        }
    </script>


    <iframe   width="100%" onload="resizeIframe(this)" src=" {{ path('list') }}{% if app.request.query.get("sender") is not null %}?sender={{ app.request.query.get("sender") }}{% endif %}"></iframe>


{% endblock %}